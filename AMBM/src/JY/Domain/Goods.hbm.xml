<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 <hibernate-mapping package="JY.Domain">
 	<class name="Goods" table="AMBM_goods">
 		<id name="good_id">
 		<generator class="native"></generator>
 		 </id>
 		
 		<property name="good_name" ></property>
 		<property name="good_description" ></property>
 		<property name="good_price"></property>
 		<property name="good_hot"></property>
 		<property name="good_new" ></property>
 		<property name="good_size" ></property>
 		<property name="good_image" ></property>
 		<property name="good_model" ></property>


 
 	</class>
 </hibernate-mapping>
